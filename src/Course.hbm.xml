<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.Course" table="course" schema="hibernate">
        <id name="maMon">
            <column name="maMon" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="tenMon">
            <column name="tenMon" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="soTinChi">
            <column name="soTinChi" sql-type="int" not-null="true"/>
        </property>
        <property name="gv">
            <column name="GV" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="phongHoc">
            <column name="phongHoc" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="dayofWeek">
            <column name="dayofWeek" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="ca">
            <column name="Ca" sql-type="int" not-null="true"/>
        </property>
        <property name="maxSlot">
            <column name="maxSlot" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>