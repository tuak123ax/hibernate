<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.Teacher" table="teacher" schema="hibernate">
        <id name="mgv">
            <column name="MGV" sql-type="int"/>
        </id>
        <property name="tenGv">
            <column name="TenGV" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="tuoi">
            <column name="Tuoi" sql-type="int" not-null="true"/>
        </property>
        <property name="diachi">
            <column name="Diachi" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="ghichu">
            <column name="Ghichu" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="hinhanh">
            <column name="Hinhanh" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="username">
            <column name="Username" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>