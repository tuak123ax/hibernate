<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.Clazz" table="class" schema="hibernate">
        <id name="tenLop">
            <column name="tenLop" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="slsv">
            <column name="SLSV" sql-type="int" not-null="true"/>
        </property>
        <property name="slNam">
            <column name="SLNam" sql-type="int" not-null="true"/>
        </property>
        <property name="slNu">
            <column name="SLNu" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>