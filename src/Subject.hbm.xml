<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.Subject" table="subject" schema="hibernate">
        <id name="maMh">
            <column name="maMH" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="tenMh">
            <column name="tenMH" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="tinchi">
            <column name="tinchi" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>